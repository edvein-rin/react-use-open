{
  "name": "react-use-open",
  "version": "1.0.0",
  "description": "A simple React hook for managing open/closed states of components.",
  "author": "Eduard Tsakhlo <edveinrin@gmail.com> (https://github.com/edvein-rin)",
  "keywords": [
    "react",
    "react-hooks",
    "state"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edvein-rin/react-use-open.git"
  },
  "bugs": {
    "url": "https://github.com/edvein-rin/react-use-open/issues"
  },
  "homepage": "https://github.com/edvein-rin/react-use-open#readme",
  "main": "./dist/react-use-open.umd.js",
  "module": "./dist/react-use-open.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/react-use-open.es.js",
      "require": "./dist/react-use-open.umd.js"
    }
  },
  "scripts": {
    "dev": "vite --config vite.config.ts --port 3000",
    "prebuild": "rm -rf ./dist",
    "build": "vite build --config vite.config.ts"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/node": "22.14.0",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@vitejs/plugin-react": "4.3.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.8.3",
    "vite": "6.2.5",
    "vite-plugin-dts": "4.5.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@10.8.0",
  "publishConfig": {
    "access": "public"
  }
}
